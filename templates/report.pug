//- template.pug
html
    head
        style
            include style.css

h1(class='title') Regressor Comparison Report

div(class='wrapper')
    each report in reportsData
        div
            h3(class='header') #{report.label}
            a(class='snap' href=report.baseline)
                img(class='screenshot', src=report.baseline)
            a(class='snap' href=report.latest)
                img(class='screenshot', src=report.latest)
            a(class='snap' href=report.comparison)
            img(class='screenshot', src=report.comparison)